<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gz.javastudy.mybatisapp.repository.StudentDao">
  <select id="selectStudent" resultType="com.gz.javastudy.mybatisapp.domain.Student">
    select * from t_mybatis_student where name = #{name}
  </select>
</mapper>